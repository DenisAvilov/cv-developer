{  
    
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "rules": {
        "linebreak-style":"off",
        "semi":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "operator-linebreak":"off"
        
    },
    "extends":  ["eslint:recommended", "google"],
    "parserOptions": {
        "sourceType": "module",
        // "allowImportExportEverywhere": true,
        "codeFrame": true,
        "ecmaVersion": 8
    }
}